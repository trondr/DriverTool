    <WarnOn>3390;$(WarnOn)</WarnOn>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>

  <ItemGroup>    
    <None Include="..\DriverTool\App.config" Link="App.config" />    
