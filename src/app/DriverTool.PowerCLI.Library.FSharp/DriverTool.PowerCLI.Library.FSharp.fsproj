	<ProjectReference Include="..\DriverTool.Library\DriverTool.Library.fsproj" />
    <ProjectReference Include="..\DriverTool.PowerCLI.Library.CSharp\DriverTool.PowerCLI.Library.CSharp.csproj" />
  </ItemGroup>
  
</Project>
